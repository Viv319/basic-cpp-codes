#include <iostream>
using namespace std;

int main() {
	//code
	int n,t,m,i=0,s=0,sum=0;
	cin>>t;
	while(t--)
	{
		sum=0,s=0;
	    cin>>n;
	    int a[n];
	    for(i=0;i<n-1;i++)
	    cin>>a[i];
	    
	    for(i=0;i<n-1;i++)
	        sum=sum+a[i];
	        
	    s=(n*(n+1)/2);
	    
	    s=s-sum;
	    cout<<s<<endl;
	}
	return 0;
}
